# Database
DATABASE_URL=postgresql+asyncpg://testboost:testboost@localhost:5433/testboost

# API
API_KEY=your-api-key-here
API_HOST=0.0.0.0
API_PORT=8000

# LLM Providers
MODEL=google-genai/gemini-2.5-flash-preview-09-2025
GOOGLE_API_KEY=your-google-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
OPENAI_API_KEY=your-openai-api-key

# vLLM / Local LLM Provider (SDIA Qwen3-Coder)
# To use vLLM, set LLM_PROVIDER=vllm or MODEL=vllm/model-name
LLM_PROVIDER=vllm
VLLM_API_BASE=https://codeia.dev.etat-ge.ch/v1
VLLM_API_KEY=EMPTY
VLLM_MODEL=/data/sdia/downloaded_models/Qwen3-Coder-30B-A3B-Instruct/

# LangSmith Observability
LANGSMITH_API_KEY=your-langsmith-api-key
LANGSMITH_PROJECT=testboost
LANGSMITH_TRACING=true

# Application
LOG_LEVEL=INFO
DEBUG=false

# Session Management
SESSION_RETENTION_DAYS=365
PROJECT_LOCK_TIMEOUT_SECONDS=3600

# Docker
DOCKER_HOST=unix:///var/run/docker.sock
MAVEN_CACHE_PATH=/tmp/maven-cache
